{
  "cli": {
    "version": ">= 0.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "development",
      "android" : {
        "buildType": "apk"
      },
      "ios" : {
        "buildType": "ipa"
      }
    },
    "SIT": {
      "developmentClient": false,
      "distribution": "SIT",
      "android" : {
        "buildType": "apk"
      },
      "ios" : {
        "buildType": "ipa"
      }
    },
    "UAT": {
      "developmentClient": false,
      "distribution": "UAT",
      "android" : {
        "buildType": "apk"
      },
      "ios" : {
        "buildType": "ipa"
      }
    },
    "review": {
      "developmentClient": false,
      "distribution": "review",
      "android" : {
        "buildType": "apk"
      },
      "ios" : {
        "buildType": "ipa"
      }
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "distribution": "internal",
      "android" : {
        "buildType": "apk"
      },
      "ios" : {
        "buildType": "ipa"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
